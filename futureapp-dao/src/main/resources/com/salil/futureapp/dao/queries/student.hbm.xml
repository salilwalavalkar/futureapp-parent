<?xml version="1.0" encoding="utf-8"?>
<!-- #HIBERNATE -->
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <query name="getAllStudentList">
        <![CDATA[
        	FROM StudentEVO
     ]]>
    </query>
    
    <query name="getStudentList">
        <![CDATA[
        	FROM StudentEVO s 
        	where s.studId = :sid
     ]]>
    </query>    

</hibernate-mapping>